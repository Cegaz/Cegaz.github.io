<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Historical
 *
 * @ORM\Table(name="historical", indexes={@ORM\Index(name="id_student", columns={"id_student"})})
 * @ORM\Entity
 */
class Historical
{
    /**
     * @var \DateTime
     *
     * @ORM\Column(name="intervention", type="datetime", nullable=false)
     */
    private $intervention = 'CURRENT_TIMESTAMP';

    /**
     * @var integer
     *
     * @ORM\Column(name="id_student", type="integer", nullable=false)
     */
    private $idStudent;

    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;



    /**
     * Set intervention
     *
     * @param \DateTime $intervention
     *
     * @return Historical
     */
    public function setIntervention($intervention)
    {
        $this->intervention = $intervention;

        return $this;
    }

    /**
     * Get intervention
     *
     * @return \DateTime
     */
    public function getIntervention()
    {
        return $this->intervention;
    }

    /**
     * Set idStudent
     *
     * @param integer $idStudent
     *
     * @return Historical
     */
    public function setIdStudent($idStudent)
    {
        $this->idStudent = $idStudent;

        return $this;
    }

    /**
     * Get idStudent
     *
     * @return integer
     */
    public function getIdStudent()
    {
        return $this->idStudent;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
